def R_Ua():
	a = "[FBAN/FB4A;FBAV/"+str(random.randint(11,77))+'.0.0.'+str(random.randrange(9,49))+'.'+str(random.randint(11,77)) +";FBBV/"+str(random.randint(11111111,99999999))+";"
	b = "[FBAN/Orca-Android;FBAV/196.0.0.50;FBPN/com.facebook.orca;FBLC/en_CA;FBBV/783193041;FBCR/VIRGIN;FBMF/Xiaomi;FBBD/Xiaomi;FBDV/2109119DI;FBSV/8.3.5;FBCA/armeabi-v7a:armeabi;FBDM/{density=2.0,width=1280,height=720};FB_FW/1;FBRV/942807972;]"
